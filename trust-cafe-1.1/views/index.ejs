    <% include ./header %>
    <!-- Page Header -->
    <!-- Set your background image for this header on the line below. -->
    <header class="intro-header" style="background-image: url('./images/home-page.jpg')">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
                    <div class="site-heading">
                        <h1>建置中.</h1>
                        <hr class="small">
                        <span class="subheading">Just a homework.</span>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <!-- Main Content -->
      <div class="container">
          <div class="row">
              <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
                <% eventList.forEach(function(event) { %>
                      <div class="post-preview">
                          <a href="/event/<%= event.id %>">
                              <h2 class="post-title">
                                <%= event.eventName %>
                              </h2>
                          </a>
                          <p>參加成員：<%= event.customerName%></p>
                          <p><%= event.date%></p>
                          <!-- <p class="post-meta">Posted by <a href="#"><%#= article.name %></a> on<%#= article.createdAt %></p> -->
                      </div>
                      <hr>
                <%  }); %>
                <!-- Pager -->
                <ul class="pager">
                    <li class="next">
                        <a href="#">Older Posts &rarr;</a>
                    </li>
                </ul>
            </div>
        </div>
    </div>

    <hr>
    <% include ./footer %>
